									"});",
									"",
									"",
									"var title = JSON.parse(responseBody);",
									"",
									"tests['Есть поле \"id\"'] = title.hasOwnProperty('id');",
									"tests['Поле \"id\" это число'] = Number.isInteger(title.id);",
									"",
									"tests['Есть поле \"name\"'] = title.hasOwnProperty('name');",
									"",
									"tests['Есть поле \"year\"'] = title.hasOwnProperty('year');",
									"tests['Поле \"year\" это число'] = Number.isInteger(title.year);",
									"",
									"tests['Есть поле \"rating\"'] = title.hasOwnProperty('rating');",
									"tests['Поле \"rating\" это число'] = Number.isInteger(title.rating);",
									"",
									"tests['Есть поле \"description\"'] = title.hasOwnProperty('description');",
									"",
									"tests['Есть поле \"genre\"'] = title.hasOwnProperty('genre');",
									"tests['Поле \"genre\" это список'] = Array.isArray(title.genre);",
									"",
									"var genre = title.genre[0];",
									"tests['Есть поле \"name\" в словаре списка \"genre\"'] = genre.hasOwnProperty('name');",
									"tests['Есть поле \"slug\" в словаре списка \"genre\"'] = genre.hasOwnProperty('slug');",
									"",
									"tests['Есть поле \"category\"'] = title.hasOwnProperty('category');",
									"",
